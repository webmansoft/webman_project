    "aliyuncs/oss-sdk-php": "^2.7",
    "qcloud/cos-sdk-v5": "^2.6",
    "qiniu/php-sdk": "^7.12",
    "firebase/php-jwt": "^6.10",
    "ext-simplexml": "*",
    "ext-pdo": "*",
    "ext-gd": "*",
    "ext-zlib": "*",
    "ext-curl": "*",
    "ext-redis": "*",
    "ext-zip": "*"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app"
    },
    "psr-0": {
      "": "extend/"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  }
