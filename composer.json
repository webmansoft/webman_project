    "webmansoft/jwt": "^1.0",
    "workerman/webman-framework": "^1.5",
    "workerman/crontab": "^1.0",
    "webman/console": "^1.3",
    "webman/event": "^1.0",
    "webman/captcha": "^1.0",
    "webman/blade": "^1.5",
    "monolog/monolog": "^2.9",
    "ramsey/uuid": "^4.7",
    "vlucas/phpdotenv": "^5.6",
    "symfony/cache": "^6.4",
    "symfony/translation": "^5.4",
    "illuminate/redis": "^10.48",
    "illuminate/database": "^10.48",
    "illuminate/pagination": "^10.48",
    "illuminate/events": "^10.48",
